{
  "expo": {
    "name": "BlueFruitNutrition",
    "slug": "BlueFruitNutrition",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoBlueFrui.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "backgroundColor": "#0C133F",
    "splash": {
      "image": "./assets/logoBlueFrui.png",
      "resizeMode": "contain",
      "backgroundColor": "#0C133F"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#0C133F"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoBlueFrui.png",
        "backgroundColor": "#0C133F"
      },
      "edgeToEdgeEnabled": true,
      "backgroundColor": "#0C133F"
    },
    "web": {
      "favicon": "./assets/logoBlueFrui.png",
      "backgroundColor": "#0C133F"
    },
    "plugins": [
      "expo-mail-composer"
    ]
  }
}
